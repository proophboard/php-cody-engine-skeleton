{
  "name": "mycompany/my-project",
  "description": "",
  "type": "project",
  "license": "proprietary",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "event-engine/cockpit-php-backend": "dev-master",
    "event-engine/discolight": "^v0.3",
    "event-engine/php-data": "^v2.0.0",
    "event-engine/php-engine": "^v0.16",
    "event-engine/php-engine-utils": "^v0.2.0",
    "event-engine/php-json-schema": "^v2.1.0",
    "event-engine/php-logger": "^v0.2.1",
    "event-engine/php-messaging": "^v0.2.0",
    "event-engine/php-persistence": "^v0.9.0",
    "event-engine/php-postgres-document-store": "^v0.12.1",
    "event-engine/php-schema": "^v0.2.0",
    "event-engine/prooph-v7-event-store": "^v0.10.0",
    "guzzlehttp/guzzle": "^7.3",
    "laminas/laminas-config-aggregator": "^1.5.0",
    "laminas/laminas-diactoros": "^2.6.0",
    "laminas/laminas-httphandlerrunner": "^2.0.0",
    "laminas/laminas-stdlib": "^3.5.0",
    "laminas/laminas-stratigility": "^3.4.0",
    "mezzio/mezzio-helpers": "^5.6.0",
    "mezzio/mezzio-problem-details": "^1.4",
    "monolog/monolog": "^2.3.1",
    "nikic/fast-route": "^v1.3.0",
    "opis/json-schema": "^1.1.0",
    "prooph/pdo-event-store": "^v1.13.0",
    "psr/http-server-middleware": "^1.0.1",
    "psr/log": "^1.1.4",
    "symfony/console": "^5.3"
  },
  "require-dev": {
    "laminas/laminas-coding-standard": "^2.2.0",
    "phpspec/prophecy": "^1.13.0",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpunit/phpunit": "^9.5.6",
    "psalm/plugin-phpunit": "^0.16.1",
    "roave/security-advisories": "dev-latest as dev-master",
    "vimeo/psalm": "^4.8"
  },
  "autoload": {
    "psr-4": {
      "Acme\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AcmeTest\\": "test/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "check": [
      "@cs-check",
      "@test",
      "static-analysis"
    ],
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "static-analysis" : "psalm --shepherd --stats"
  }
}
